<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Module SYSTEM "file:/Repository/Level2/Soft/ProC/moduledef.dtd">
<module>
<intro>
<modulename name="chop"/>
<author name="Andrew Walker"/>
<description text="Chops a given data set into odd, even, difference, and Index data sets."/>
<version minor="1" major="0"/>
<state devstate="release"/>
</intro>

<interface>
<input>
<table type="float" name="Array" descr="The array of data values." />
</input>

<output>
<table type="float" name="Odd Array" descr="The odd array." />
</output>
<output>
<table type="float" name="Even Array" descr="The even array." />
</output>
<output>
<table type="float" name="Difference Array" descr="The difference array." />
</output>
<output>
<table type="float" name="Index Array" descr="The index array." />
</output>
</interface>
</module>

