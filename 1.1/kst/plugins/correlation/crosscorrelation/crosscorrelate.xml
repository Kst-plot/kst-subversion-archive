<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Module SYSTEM "file:/Repository/Level2/Soft/ProC/moduledef.dtd">
<module>
<intro>
<modulename name="crosscorrelate" readableName="Cross-correlation"/>
<author name="Andrew Walker"/>
<description text="Generates the correlation of one vector with another."/>
<version minor="1" major="0"/>
<state devstate="release"/>
</intro>

<interface>
<input>
<table type="float" name="Array One" descr="The first array to be correlated." />
</input>
<input>
<table type="float" name="Array Two" descr="The second array to be correlated." />
</input>

<output>
<table type="float" name="Step value" descr="The step value." />
</output>
<output>
<table type="float" name="Correlated" descr="The correlated array." />
</output>
</interface>
</module>

