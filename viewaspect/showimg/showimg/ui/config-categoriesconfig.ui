<connections>
    <connection>
        <sender>enableCategoriesCheckBox</sender>
        <signal>toggled(bool)</signal>
        <receiver>addAllImageCheckBox</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
    <connection>
        <sender>enableCategoriesCheckBox</sender>
        <signal>toggled(bool)</signal>
        <receiver>categoriesOptionsTabWidget</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
    <connection>
        <sender>enableCategoriesCheckBox</sender>
        <signal>toggled(bool)</signal>
        <receiver>categoriesTextLabel</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
    <connection>
        <sender>enableCategoriesCheckBox</sender>
        <signal>toggled(bool)</signal>
        <receiver>categoriesComboBox</receiver>
        <slot>setEnabled(bool)</slot>
    </connection>
</connections>
